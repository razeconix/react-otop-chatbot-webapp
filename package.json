  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dialogflow": "^1.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "mongoose": "^5.9.27"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
  
}
